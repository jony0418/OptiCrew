<div class="employeeBackground">
<div class="employeeContainer">
<h1>WELCOME, {{name}}</h1>

<div class="appCont">
  <div class="icon">
    <img src="/img/icons/sunbed.png" alt="Icon 1">
    <button>HOLIDAYS</button>
  </div>
  <div class="icon">
    <img src="/img/icons/division.png" alt="Icon 2">
    <button id="appBtn">SEE ALL EMPLOYEES</button>
  </div>
    <div class="icon">
    <img src="/img/icons/employee.png" alt="Icon 3">
    <button>ADD EMPLOYEE</button>
  </div>
  <div class="icon">
    <img src="/img/icons/medical-history.png" alt="Icon 4">
    <button>SICK LEAVE</button>
  </div>
</div>



<div>
